Fe = 8000;                          % Frequence d'echantillonnage
t = 0:1/Fe:0.05*((Fe-1)/Fe);        % Vecteur de temps
y = zeros(size(t));

% Fonction porte
for n = 1:length(t)
    if ((t(n)>0) && (t(n)<0.05))
        y(n) = 1;
    else
        y(n) = 0;
    end
end

figure
plot(t,y)
axis([-0.05 0.1